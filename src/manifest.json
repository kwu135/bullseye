{
  "manifest_version": 2,

  "name": "Bullseye",
  "description": "This extension is to help others improve productivity",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
    // "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}